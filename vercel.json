{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      },
      {
        "src": "/swagger",
        "dest": "/swagger/index.html"
      },
      {
        "src": "/swagger/(.*)",
        "dest": "/swagger/$1"
      }
    ]
  }
  